net: "bsds500_conv_after.prototxt"
test_iter: 200
test_interval: 500

base_lr: 0.000000001
lr_policy: "fixed"
gamma: 0.0001

display: 100
max_iter: 4000
momentum: 0.8
weight_decay: 0.001
snapshot: 4000
snapshot_prefix: "../model/bsds500_conv_after"
# uncomment the following to default to CPU mode solving
solver_mode: GPU
