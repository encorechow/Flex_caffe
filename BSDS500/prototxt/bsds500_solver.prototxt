net: "bsds500_new_2block.prototxt"
test_iter: 200
test_interval: 500

base_lr: 0.0000001
lr_policy: "fixed"
gamma: 0.0001

display: 100
max_iter: 4000
momentum: 0.9
weight_decay: 0.001
snapshot: 100
snapshot_prefix: "../model/bsds500_new_2block2"
# uncomment the following to default to CPU mode solving
solver_mode: GPU
